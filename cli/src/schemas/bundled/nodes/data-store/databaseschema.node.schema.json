{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DatabaseSchema",
  "description": "Logical grouping of database objects",
  "allOf": [
    {
      "$ref": "../../base/spec-node.schema.json"
    }
  ],
  "properties": {
    "spec_node_id": {
      "const": "data-store.databaseschema"
    },
    "layer_id": {
      "const": "data-store"
    },
    "type": {
      "const": "databaseschema"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "name": {
          "type": "string"
        },
        "owner": {
          "type": "string"
        },
        "tables": {
          "type": "array",
          "description": "Contains relationship"
        },
        "views": {
          "type": "array",
          "description": "Contains relationship"
        },
        "functions": {
          "type": "array",
          "description": "Contains relationship"
        },
        "sequences": {
          "type": "array",
          "description": "Contains relationship"
        }
      },
      "additionalProperties": false
    }
  }
}
