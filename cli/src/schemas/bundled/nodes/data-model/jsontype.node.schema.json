{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "JSONType",
  "description": "Core JSON data types",
  "allOf": [
    {
      "$ref": "../../base/spec-node.schema.json"
    }
  ],
  "properties": {
    "spec_node_id": {
      "const": "data-model.jsontype"
    },
    "layer_id": {
      "const": "data-model"
    },
    "type": {
      "const": "jsontype"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "value": {
          "type": "string"
        }
      },
      "required": ["value"],
      "additionalProperties": false
    }
  }
}
