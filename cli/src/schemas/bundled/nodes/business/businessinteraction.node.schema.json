{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "BusinessInteraction",
  "description": "A behavior element representing collective behavior performed by a BusinessCollaboration. It is the interaction counterpart to BusinessProcess (performed by a role) and BusinessFunction (performed by an actor).",
  "allOf": [
    {
      "$ref": "../../base/spec-node.schema.json"
    }
  ],
  "properties": {
    "spec_node_id": {
      "const": "business.businessinteraction"
    },
    "layer_id": {
      "const": "business"
    },
    "type": {
      "const": "businessinteraction"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "name": {
          "type": "string"
        },
        "documentation": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  }
}
