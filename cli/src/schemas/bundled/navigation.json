    "guardaction": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "GuardAction",
      "description": "Defines the response executed by a NavigationGuard when its condition evaluates to false (access denied). Specifies whether to redirect the user, block navigation in place, notify with a message, or prompt for confirmation — along with the target destination and whether to preserve the attempted route for post-authentication redirect.",
      "allOf": [
        {
          "$ref": "urn:dr:spec:base:spec-node"
        }
      ],
      "properties": {
        "spec_node_id": {
          "const": "navigation.guardaction"
        },
        "layer_id": {
          "const": "navigation"
        },
        "type": {
          "const": "guardaction"
        },
        "attributes": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string",
              "format": "uuid"
            },
            "name": {
              "type": "string"
            },
            "action": {
              "type": "string",
              "enum": ["redirect", "block", "notify", "prompt"],
              "description": "The action taken when the guard denies access to the target route."
            },
            "target": {
              "type": "string",
              "description": "Destination for redirect or replace actions — a Route ID (e.g., 'login') or an absolute URL. Ignored for block and notify actions."
            },
            "message": {
              "type": "string",
              "description": "User-facing message shown when action is 'notify' or 'prompt', explaining why access was denied."
            },
            "preserveRoute": {
              "type": "boolean",
              "description": "When true, the originally attempted route is stored (e.g., in session state) so the user can be redirected back to it after successfully authenticating."
            }
          },
          "required": ["action", "target", "message", "preserveRoute"],
          "additionalProperties": false
        }
      },
      "$id": "urn:dr:spec:node:navigation.guardaction"
    },
    "guardcondition": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "GuardCondition",
      "description": "Boolean predicate evaluated by a NavigationGuard to determine whether route access should be permitted. Expressions may reference user session state, roles, or feature flags. Async conditions (e.g., token validation API calls) are supported with a configurable timeout; on timeout, the condition fails closed (access denied) to preserve security.",
      "allOf": [
        {
          "$ref": "urn:dr:spec:base:spec-node"
        }
      ],
      "properties": {
        "spec_node_id": {
          "const": "navigation.guardcondition"
        },
        "layer_id": {
          "const": "navigation"
        },
        "type": {
          "const": "guardcondition"
        },
        "attributes": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string",
              "format": "uuid"
            },
            "name": {
              "type": "string"
            },
            "expression": {
              "type": "string",
              "description": "Boolean predicate expression to evaluate — may be a JavaScript expression, CEL expression, or logical reference to a named predicate (e.g., 'user.isAuthenticated && user.roles.includes(route.meta.roles)')."
            },
            "async": {
              "type": "boolean",
              "description": "When true, the expression is evaluated asynchronously (e.g., via an API call). The guard will await resolution up to the timeout limit before failing closed."
            },
            "timeout": {
              "type": "integer",
              "description": "Maximum milliseconds to wait for an async condition to resolve. On timeout, the guard treats the condition as false (access denied) to fail securely."
            }
          },
          "required": ["expression", "timeout"],
          "additionalProperties": false
        }
      },
      "$id": "urn:dr:spec:node:navigation.guardcondition"
    },
