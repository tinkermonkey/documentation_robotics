    "breadcrumbconfig": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "BreadcrumbConfig",
      "description": "Configuration for breadcrumb navigation display, specifying path generation rules, separator styles, truncation behavior, and home link settings. Applied globally to define breadcrumb rendering for a navigation scope rather than attached per individual Route. Provides users with location context and navigation history.",
      "allOf": [
        {
          "$ref": "urn:dr:spec:base:spec-node"
        }
      ],
      "properties": {
        "spec_node_id": {
          "const": "navigation.breadcrumbconfig"
        },
        "layer_id": {
          "const": "navigation"
        },
        "type": {
          "const": "breadcrumbconfig"
        },
        "attributes": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string",
              "format": "uuid"
            },
            "name": {
              "type": "string"
            },
            "label": {
              "type": "string"
            },
            "labelTemplate": {
              "type": "string",
              "description": "Template string for dynamic label generation using {title} placeholder syntax to interpolate route metadata. Used when breadcrumb labels need to vary by route context rather than a static label."
            },
            "showHome": {
              "type": "boolean"
            },
            "homeLabel": {
              "type": "string"
            },
            "homeRoute": {
              "type": "string"
            }
          },
          "required": ["label"],
          "additionalProperties": false
        }
      },
      "$id": "urn:dr:spec:node:navigation.breadcrumbconfig"
    },
    "contextvariable": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "ContextVariable",
      "description": "A typed variable shared across steps of a NavigationFlow, enabling state to persist and transfer between route transitions.",
