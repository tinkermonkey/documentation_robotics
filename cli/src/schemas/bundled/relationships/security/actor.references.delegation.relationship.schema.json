{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Actor References Delegation",
  "description": "Defines relationship: security.actor references security.delegation",
  "allOf": [
    {
      "$ref": "../../base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "security.actor.references.security.delegation"
    },
    "source_spec_node_id": {
      "const": "security.actor"
    },
    "source_layer": {
      "const": "security"
    },
    "destination_spec_node_id": {
      "const": "security.delegation"
    },
    "destination_layer": {
      "const": "security"
    },
    "predicate": {
      "const": "references"
    },
    "cardinality": {
      "const": "many-to-one"
    },
    "strength": {
      "const": "medium"
    }
  }
}
