{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Driver Influence Principle",
  "description": "Defines relationship: motivation.driver influence motivation.principle",
  "allOf": [
    {
      "$ref": "../../base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "motivation.influence.motivation"
    },
    "source_spec_node_id": {
      "const": "motivation.driver"
