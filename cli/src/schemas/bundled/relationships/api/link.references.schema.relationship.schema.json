{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/tinkermonkey/documentation_robotics/spec/relationships/api/link.references.schema.relationship.schema.json",
  "title": "Link References Schema",
  "description": "Defines relationship: api.link references api.schema",
  "allOf": [
    {
      "$ref": "../../schemas/base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "api.references.api"
    },
    "source_spec_node_id": {
      "const": "api.link"
    },
    "source_layer": {
      "const": "api"
    },
    "destination_spec_node_id": {
      "const": "api.schema"
    },
    "destination_layer": {
      "const": "api"
    },
    "predicate": {
      "const": "references"
    },
    "cardinality": {
      "const": "many-to-many"
    },
    "strength": {
      "const": "medium"
    }
  }
}
