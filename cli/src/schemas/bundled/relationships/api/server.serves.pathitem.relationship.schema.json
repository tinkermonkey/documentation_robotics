{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Server serves Pathitem",
  "description": "Defines relationship: api.server serves api.pathitem",
  "allOf": [
    {
      "$ref": "../../base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "api.server.serves.api.pathitem"
    },
    "source_spec_node_id": {
      "const": "api.server"
    },
    "source_layer": {
      "const": "api"
    },
    "destination_spec_node_id": {
      "const": "api.pathitem"
    },
    "destination_layer": {
      "const": "api"
    },
    "predicate": {
      "const": "serves"
    },
    "cardinality": {
      "const": "many-to-one"
    },
    "strength": {
      "const": "medium"
    }
  }
}
