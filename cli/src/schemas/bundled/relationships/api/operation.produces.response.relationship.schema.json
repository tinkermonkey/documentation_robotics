{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Operation produces Response",
  "description": "Defines relationship: api.operation produces api.response",
  "allOf": [
    {
      "$ref": "../../base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "api.operation.produces.api.response"
    },
    "source_spec_node_id": {
      "const": "api.operation"
    },
    "source_layer": {
      "const": "api"
    },
    "destination_spec_node_id": {
      "const": "api.response"
    },
    "destination_layer": {
      "const": "api"
    },
    "predicate": {
      "const": "produces"
    },
    "cardinality": {
      "const": "many-to-one"
    },
    "strength": {
      "const": "medium"
    }
  }
}
