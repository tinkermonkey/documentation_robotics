{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Paths references Tag",
  "description": "Defines relationship: api.paths references api.tag",
  "allOf": [
    {
      "$ref": "../../base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "api.paths.references.api.tag"
    },
    "source_spec_node_id": {
      "const": "api.paths"
    },
    "source_layer": {
      "const": "api"
    },
    "destination_spec_node_id": {
      "const": "api.tag"
    },
    "destination_layer": {
      "const": "api"
    },
    "predicate": {
      "const": "references"
    },
    "cardinality": {
      "const": "many-to-one"
    },
    "strength": {
      "const": "medium"
    }
  }
}
