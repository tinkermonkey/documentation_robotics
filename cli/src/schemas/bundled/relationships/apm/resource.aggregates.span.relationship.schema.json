  "title": "Resource Aggregates Span",
  "description": "Defines relationship: apm.resource aggregates apm.span",
  "allOf": [
    {
      "$ref": "../../schemas/base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "apm.aggregates.apm"
    },
