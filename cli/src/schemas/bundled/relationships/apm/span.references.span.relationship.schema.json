{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Span References Span",
  "description": "Defines relationship: apm.span references apm.span",
  "allOf": [
    {
      "$ref": "../../base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "apm.references.apm"
    },
    "source_spec_node_id": {
      "const": "apm.span"
    },
    "source_layer": {
      "const": "apm"
    },
    "destination_spec_node_id": {
      "const": "apm.span"
    },
    "destination_layer": {
      "const": "apm"
    },
    "predicate": {
      "const": "references"
    },
    "cardinality": {
      "const": "many-to-many"
    },
    "strength": {
      "const": "medium"
    }
  }
}
