{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Field triggers Eventhandler",
  "description": "Defines relationship: data-store.field triggers data-store.eventhandler",
  "allOf": [
    {
      "$ref": "../../base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "data-store.field.triggers.data-store.eventhandler"
    },
    "source_spec_node_id": {
      "const": "data-store.field"
    },
    "source_layer": {
      "const": "data-store"
    },
    "destination_spec_node_id": {
      "const": "data-store.eventhandler"
    },
    "destination_layer": {
      "const": "data-store"
    },
    "predicate": {
      "const": "triggers"
    },
    "cardinality": {
      "const": "many-to-one"
    },
    "strength": {
      "const": "medium"
    }
  }
}
