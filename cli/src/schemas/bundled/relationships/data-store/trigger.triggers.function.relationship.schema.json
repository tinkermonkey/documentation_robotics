{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Trigger Triggers Function",
  "description": "Defines relationship: data-store.trigger triggers data-store.function",
  "allOf": [
    {
      "$ref": "../../base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "data-store.triggers.data-store"
    },
    "source_spec_node_id": {
      "const": "data-store.trigger"
    },
    "source_layer": {
      "const": "data-store"
    },
    "destination_spec_node_id": {
      "const": "data-store.function"
    },
    "destination_layer": {
      "const": "data-store"
    },
    "predicate": {
      "const": "triggers"
    },
    "cardinality": {
      "const": "many-to-many"
    },
    "strength": {
      "const": "medium"
    }
  }
}
