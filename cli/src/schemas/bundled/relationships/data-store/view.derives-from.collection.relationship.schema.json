{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "View derives-from Collection",
  "description": "Defines relationship: data-store.view derives-from data-store.collection",
  "allOf": [
    {
      "$ref": "../../base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "data-store.view.derives-from.data-store.collection"
    },
    "source_spec_node_id": {
      "const": "data-store.view"
    },
    "source_layer": {
      "const": "data-store"
    },
    "destination_spec_node_id": {
      "const": "data-store.collection"
    },
    "destination_layer": {
      "const": "data-store"
    },
    "predicate": {
      "const": "derives-from"
    },
    "cardinality": {
      "const": "many-to-one"
    },
    "strength": {
      "const": "medium"
    }
  }
}
