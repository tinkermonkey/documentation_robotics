{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Database Composes Column",
  "description": "Defines relationship: data-store.database composes data-store.column",
  "allOf": [
    {
      "$ref": "../../base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "data-store.database.composes.data-store.column"
    },
    "source_spec_node_id": {
      "const": "data-store.database"
    },
    "source_layer": {
      "const": "data-store"
    },
    "destination_spec_node_id": {
      "const": "data-store.column"
    },
    "destination_layer": {
      "const": "data-store"
    },
    "predicate": {
      "const": "composes"
    },
    "cardinality": {
      "const": "many-to-many"
    },
    "strength": {
      "const": "medium"
    }
  }
}
