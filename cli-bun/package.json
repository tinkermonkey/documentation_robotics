{
  "name": "@doc-robotics/cli-bun",
  "version": "0.1.0",
  "description": "Bun CLI for Documentation Robotics",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "dr": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "bun run src/cli.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "lint": "biome lint src tests",
    "format": "biome format --write src tests",
    "check": "biome check src tests"
  },
  "keywords": [
    "architecture",
    "documentation",
    "modeling",
    "federated"
  ],
  "author": "Documentation Robotics Contributors",
  "license": "MIT",
  "dependencies": {
    "yaml": "^2.4.0"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^3.4.2",
    "@types/bun": "^1.3.5",
    "prettier": "^3.7.4",
    "typescript": "^5.3.0"
  }
}
