  "title": "Operation Aggregates Server",
  "description": "Defines relationship: api.operation aggregates api.server",
  "allOf": [
    {
      "$ref": "../../schemas/base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "api.aggregates.api"
    },
    "source_spec_node_id": {
