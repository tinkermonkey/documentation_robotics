  "title": "Threat influence Security Policy",
  "description": "Defines relationship: security.threat influence security.securitypolicy",
  "allOf": [
    {
      "$ref": "../../base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "security.threat.influence.security.securitypolicy"
    },
    "source_spec_node_id": {
      "const": "security.threat"
