{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Components Composes Paths",
  "description": "Defines relationship: api.components composes api.paths",
  "allOf": [
    {
      "$ref": "../../base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "api.composes.api"
    },
    "source_spec_node_id": {
      "const": "api.components"
    },
    "source_layer": {
      "const": "api"
    },
    "destination_spec_node_id": {
      "const": "api.paths"
    },
    "destination_layer": {
      "const": "api"
    },
    "predicate": {
      "const": "composes"
    },
    "cardinality": {
      "const": "many-to-many"
    },
    "strength": {
      "const": "medium"
    }
  }
}
