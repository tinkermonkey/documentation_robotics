{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "View embeds Subview",
  "description": "Defines relationship: ux.view embeds ux.subview",
  "allOf": [
    {
      "$ref": "../../base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "ux.view.embeds.ux.subview"
    },
    "source_spec_node_id": {
      "const": "ux.view"
    },
    "source_layer": {
      "const": "ux"
    },
    "destination_spec_node_id": {
      "const": "ux.subview"
    },
    "destination_layer": {
      "const": "ux"
    },
    "predicate": {
      "const": "embeds"
    },
    "cardinality": {
      "const": "many-to-one"
    },
    "strength": {
      "const": "medium"
    }
  }
}
