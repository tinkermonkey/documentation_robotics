{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Span Aggregates Spanlink",
  "description": "Defines relationship: apm.span aggregates apm.spanlink",
  "allOf": [
    {
      "$ref": "../../base/spec-node-relationship.schema.json"
    }
  ],
  "properties": {
    "id": {
      "const": "apm.span.aggregates.spanlink"
    },
    "source_spec_node_id": {
      "const": "apm.span"
    },
    "source_layer": {
      "const": "apm"
    },
    "destination_spec_node_id": {
      "const": "apm.spanlink"
    },
    "destination_layer": {
      "const": "apm"
    },
    "predicate": {
      "const": "aggregates"
    },
    "cardinality": {
      "const": "one-to-many"
    },
    "strength": {
      "const": "medium"
    }
  }
}
