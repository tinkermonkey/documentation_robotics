{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ArraySchema",
  "description": "ArraySchema validation rules",
  "allOf": [
    {
      "$ref": "../../base/spec-node.schema.json"
    }
  ],
  "properties": {
    "spec_node_id": {
      "const": "data-model.arrayschema"
    },
    "layer_id": {
      "const": "data-model"
    },
    "type": {
      "const": "arrayschema"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "VARCHAR",
            "TEXT",
            "INTEGER",
            "BIGINT",
            "SMALLINT",
            "DECIMAL",
            "NUMERIC",
            "BOOLEAN",
            "DATE",
            "TIMESTAMP",
            "TIMESTAMPTZ",
            "UUID",
            "JSON",
            "JSONB",
            "BYTEA",
            "ARRAY",
            "SERIAL",
            "BIGSERIAL",
            "FLOAT",
            "DOUBLE PRECISION",
            "INTERVAL",
            "INET",
            "CIDR",
            "MACADDR",
            "BIT",
            "XML",
            "MONEY",
            "POINT",
            "LINE",
            "POLYGON",
            "CIRCLE"
          ]
        },
        "items": {
          "type": "string"
        },
        "minItems": {
          "type": "integer"
        },
        "maxItems": {
          "type": "integer"
        },
        "uniqueItems": {
          "type": "boolean"
        },
        "contains": {
          "type": "string"
        }
      },
      "required": [
        "type"
      ],
      "additionalProperties": false
    }
  }
}
