{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "NumericSchema",
  "description": "NumericSchema validation rules",
  "allOf": [
    {
      "$ref": "../../base/spec-node.schema.json"
    }
  ],
  "properties": {
    "spec_node_id": {
      "const": "data-model.numericschema"
    },
    "layer_id": {
      "const": "data-model"
    },
    "type": {
      "const": "numericschema"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string"
        },
        "minimum": {
          "type": "number"
        },
        "maximum": {
          "type": "number"
        },
        "exclusiveMinimum": {
          "type": "number"
        },
        "exclusiveMaximum": {
          "type": "number"
        },
        "multipleOf": {
          "type": "number"
        }
      },
      "required": ["type"],
      "additionalProperties": false
    }
  }
}
