{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Meaning",
  "description": "Knowledge or expertise present in, or the interpretation given to, a concept in a particular context, as understood by stakeholders.",
  "allOf": [
    {
      "$ref": "../../base/spec-node.schema.json"
    }
  ],
  "properties": {
    "spec_node_id": {
      "const": "motivation.meaning"
    },
    "layer_id": {
      "const": "motivation"
    },
    "type": {
      "const": "meaning"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "documentation": {
          "type": "string"
        }
      },
      "additionalProperties": false
    }
  }
}
