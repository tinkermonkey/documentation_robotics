{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/tinkermonkey/documentation_robotics/spec/schemas/common/predicates.schema.json",
  "title": "Semantic Predicates Library",
  "description": "Definitions of semantic predicates used in Documentation Robotics relationships, aligned with ArchiMate and ontology best practices",
  "version": "1.0.0",

  "definitions": {
    "PredicateType": {
      "type": "string",
      "description": "Semantic predicate describing a relationship type. Includes both forward and inverse forms.",
      "enum": [
        "supports",
        "supported-by",
        "realizes",
        "realized-by",
        "fulfills",
        "fulfilled-by",
        "delivers",
        "delivered-by",
        "constrains",
        "constrained-by",
        "governs",
        "governed-by",
        "owns",
        "owned-by",
        "performs",
        "performed-by",
        "provides",
        "provided-by",
        "consumes",
        "consumed-by",
        "requires",
        "required-by",
        "assigns",
        "assigned-to",
        "associated-with",
        "protects",
        "protected-by",
        "mitigates",
        "mitigated-by",
        "accesses",
        "accessed-by",
        "uses",
        "used-by",
        "implements",
        "implemented-by",
        "specializes",
        "generalized-by",
        "influences",
        "influenced-by",
        "serves",
        "served-by",
        "references",
        "referenced-by",
        "depends-on",
        "dependency-of",
        "supports-goals",
        "fulfills-requirements",
        "delivers-value",
        "measures-outcome",
        "governed-by-principles",
        "enforces-requirement",
        "enforced-by",
        "traces",
        "traced-by",
        "monitors",
        "monitored-by",
        "measures",
        "measured-by",
        "authenticates",
        "authenticated-by",
        "authorizes",
        "authorized-by",
        "renders",
        "rendered-by",
        "binds-to",
        "bound-by",
        "navigates-to",
        "navigated-from",
        "maps-to",
        "mapped-from",
        "references-table",
        "table-referenced-by",
        "derives-from",
        "derived-by",
        "tests",
        "tested-by",
        "validates",
        "validated-by",
        "composes",
        "composed-of",
        "aggregates",
        "aggregated-by",
        "triggers",
        "triggered-by",
        "flows-to",
        "flows-from",
        "master-data-source",
        "master-data-for",
        "process-steps",
        "step-of-process",
        "realized-by-process",
        "realizes-process",
        "represented-by-dataobject",
        "represents",
        "archimate-ref",
        "archimate-source",
        "business-object-ref",
        "data-model-source",
        "business-interface-ref",
        "business-service-ref",
        "apm-data-quality-metrics",
        "quality-measured-by",
        "data-governance",
        "governs-data",
        "database-mapping",
        "mapped-from-schema",
        "database-column",
        "column-source",
        "database-table",
        "table-source",
        "encryption-required",
        "requires-encryption-for",
        "security-controls",
        "controls-process",
        "apm-trace",
        "traced-from",
        "traced",
        "traces-service",
        "governance-owner",
        "owned-data-governance",
        "classification",
        "separation-of-duty",
        "required-permissions",
        "security-resource"
      ]
    },

    "RelationshipStrength": {
      "type": "string",
      "description": "Strength/importance of a relationship",
      "enum": ["critical", "high", "medium", "low"]
    },

    "RelationshipCategory": {
      "type": "string",
      "description": "Category of relationship based on layer or domain",
      "enum": [
        "motivation",
        "business",
        "security",
        "application",
        "technology",
        "api",
        "data",
        "data-store",
        "ux",
        "navigation",
        "apm",
        "testing",
        "structural",
        "behavioral",
        "dependency",
        "traceability",
        "governance"
      ]
    },

    "PredicateDefinition": {
      "type": "object",
      "description": "Full definition of a semantic predicate. Field names use snake_case to match spec/predicates.json.",
      "required": ["predicate", "inverse", "category", "description"],
      "properties": {
        "predicate": {
          "$ref": "#/definitions/PredicateType",
          "description": "Forward direction predicate"
        },
        "inverse": {
          "$ref": "#/definitions/PredicateType",
          "description": "Reverse direction predicate"
        },
        "category": {
          "$ref": "#/definitions/RelationshipCategory",
          "description": "Relationship category"
        },
        "description": {
          "type": "string",
          "description": "Human-readable description of the predicate"
        },
        "archimate_alignment": {
          "type": "string",
          "description": "Corresponding ArchiMate relationship type (if applicable)",
          "enum": [
            "Composition",
            "Aggregation",
            "Assignment",
            "Realization",
            "Serving",
            "Access",
            "Influence",
            "Triggering",
            "Flow",
            "Specialization",
            "Association"
          ]
        },
        "semantics": {
          "type": "object",
          "description": "Semantic properties of the relationship",
          "required": ["directionality", "transitivity", "symmetry", "reflexivity"],
          "properties": {
            "directionality": {
              "type": "string",
              "enum": ["unidirectional", "bidirectional"],
              "description": "Whether the relationship can be navigated in both directions"
            },
            "transitivity": {
              "type": "boolean",
              "description": "Whether the relationship is transitive (if A→B and B→C, then A→C)"
            },
            "symmetry": {
              "type": "boolean",
              "description": "Whether the relationship is symmetric (if A→B, then B→A)"
            },
            "reflexivity": {
              "type": "boolean",
              "description": "Whether an element can have this relationship with itself"
            }
          }
        },
        "default_strength": {
          "$ref": "#/definitions/RelationshipStrength",
          "description": "Default strength for this predicate type"
        }
      }
    }
  },

  "predicates": {
    "supports": {
      "predicate": "supports",
      "inverse": "supported-by",
      "category": "motivation",
      "description": "Element contributes to achieving or enabling another element",
      "archimate_alignment": "Influence",
      "semantics": {
        "directionality": "bidirectional",
        "transitivity": true,
        "symmetry": false,
        "reflexivity": false
      },
      "default_strength": "high"
    },
    "realizes": {
      "predicate": "realizes",
      "inverse": "realized-by",
      "category": "structural",
      "description": "Element implements or makes concrete another element",
      "archimate_alignment": "Realization",
      "semantics": {
        "directionality": "bidirectional",
        "transitivity": true,
        "symmetry": false,
        "reflexivity": false
      },
      "default_strength": "high"
    },
    "fulfills": {
      "predicate": "fulfills",
      "inverse": "fulfilled-by",
      "category": "motivation",
      "description": "Element satisfies the needs specified by another element",
      "archimate_alignment": "Realization",
      "semantics": {
        "directionality": "bidirectional",
        "transitivity": false,
        "symmetry": false,
        "reflexivity": false
      },
      "default_strength": "high"
    },
    "delivers": {
      "predicate": "delivers",
      "inverse": "delivered-by",
      "category": "motivation",
      "description": "Element provides value or benefit",
      "archimate_alignment": "Realization",
      "semantics": {
        "directionality": "bidirectional",
        "transitivity": false,
        "symmetry": false,
        "reflexivity": false
      },
      "default_strength": "high"
    },
    "constrains": {
      "predicate": "constrains",
      "inverse": "constrained-by",
      "category": "motivation",
      "description": "Element limits or restricts another element",
      "archimate_alignment": "Influence",
      "semantics": {
        "directionality": "bidirectional",
        "transitivity": false,
        "symmetry": false,
        "reflexivity": false
      },
      "default_strength": "medium"
    },
    "governs": {
      "predicate": "governs",
      "inverse": "governed-by",
      "category": "motivation",
      "description": "Element provides rules or guidelines for another element",
      "archimate_alignment": "Influence",
      "semantics": {
        "directionality": "bidirectional",
        "transitivity": false,
        "symmetry": false,
        "reflexivity": false
      },
      "default_strength": "high"
    },
    "owns": {
      "predicate": "owns",
      "inverse": "owned-by",
      "category": "business",
      "description": "Element has responsibility or authority over another element",
      "archimate_alignment": "Assignment",
      "semantics": {
        "directionality": "bidirectional",
        "transitivity": false,
        "symmetry": false,
        "reflexivity": false
      },
      "default_strength": "medium"
    },
    "performs": {
      "predicate": "performs",
      "inverse": "performed-by",
      "category": "business",
      "description": "Element executes or carries out another element",
      "archimate_alignment": "Assignment",
      "semantics": {
        "directionality": "bidirectional",
        "transitivity": false,
        "symmetry": false,
        "reflexivity": false
      },
      "default_strength": "medium"
    },
    "provides": {
      "predicate": "provides",
      "inverse": "provided-by",
      "category": "structural",
      "description": "Element makes functionality available",
      "archimate_alignment": "Serving",
      "semantics": {
        "directionality": "bidirectional",
        "transitivity": false,
        "symmetry": false,
        "reflexivity": false
      },
      "default_strength": "high"
    },
    "consumes": {
      "predicate": "consumes",
      "inverse": "consumed-by",
      "category": "dependency",
