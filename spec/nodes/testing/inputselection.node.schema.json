{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/tinkermonkey/documentation_robotics/spec/nodes/testing/inputselection.node.schema.json",
  "title": "InputSelection",
  "description": "Specific partition value selected for a test case",
  "allOf": [
    {
      "$ref": "../../schemas/base/spec-node.schema.json"
    }
  ],
  "properties": {
    "spec_node_id": {
      "const": "testing.inputselection"
    },
    "layer_id": {
      "const": "testing"
    },
    "type": {
      "const": "inputselection"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "partitionRef": {
          "type": "string"
        },
        "selectedValue": {
          "type": "string"
        },
        "concreteValue": {
          "type": "string"
        },
        "valueSource": {
          "type": "string"
        }
      },
      "required": [
        "partitionRef",
        "selectedValue",
        "concreteValue",
        "valueSource"
      ],
      "additionalProperties": false
    }
  }
}
