{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/tinkermonkey/documentation_robotics/spec/nodes/testing/partitionvalue.node.schema.json",
  "title": "PartitionValue",
  "description": "A specific partition within the input space",
  "allOf": [
    {
      "$ref": "../../schemas/base/spec-node.schema.json"
    }
  ],
  "properties": {
    "spec_node_id": {
      "const": "testing.partitionvalue"
    },
    "layer_id": {
      "const": "testing"
    },
    "type": {
      "const": "partitionvalue"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "label": {
          "type": "string"
        },
        "constraint": {
          "type": "string"
        },
        "constraintExpression": {
          "type": "string"
        },
        "category": {
          "type": "string",
          "enum": [
            "typical",
            "boundary",
            "invalid",
            "None",
            "special"
          ]
        },
        "representativeValue": {
          "type": "string"
        }
      },
      "required": [
        "label",
        "constraint",
        "constraintExpression",
        "category",
        "representativeValue"
      ],
      "additionalProperties": false
    }
  }
}
