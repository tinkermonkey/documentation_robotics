{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/tinkermonkey/documentation_robotics/spec/nodes/testing/outcomecategory.node.schema.json",
  "title": "OutcomeCategory",
  "description": "Category of expected outcomes (not specific assertions)",
  "allOf": [
    {
      "$ref": "../../schemas/base/spec-node.schema.json"
    }
  ],
  "properties": {
    "spec_node_id": {
      "const": "testing.outcomecategory"
    },
    "layer_id": {
      "const": "testing"
    },
    "type": {
      "const": "outcomecategory"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "outcomeType": {
          "type": "string",
          "enum": [
            "success",
            "validation-error",
            "authorization-denied",
            "not-found",
            "conflict",
            "precondition-failed",
            "idempotent-noop",
            "partial-success",
            "error"
          ]
        },
        "httpStatusRange": {
          "type": "string"
        }
      },
      "required": [
        "outcomeType",
        "httpStatusRange"
      ],
      "additionalProperties": false
    }
  }
}
