{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/tinkermonkey/documentation_robotics/spec/nodes/motivation/outcome.node.schema.json",
  "title": "Outcome",
  "description": "End result that has been achieved",
  "allOf": [
    {
      "$ref": "../../schemas/base/spec-node.schema.json"
    }
  ],
  "properties": {
    "spec_node_id": {
      "const": "motivation.outcome"
    },
    "layer_id": {
      "const": "motivation"
    },
    "type": {
      "const": "outcome"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "status": {
          "type": "string",
          "enum": [
            "planned",
            "in-progress",
            "achieved",
            "not-achieved"
          ]
        }
      },
      "required": [
        "status"
      ],
      "additionalProperties": false
    }
  }
}
