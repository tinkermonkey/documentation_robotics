{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/tinkermonkey/documentation_robotics/spec/nodes/technology/artifact.node.schema.json",
  "title": "Artifact",
  "description": "Physical piece of data used or produced",
  "allOf": [
    {
      "$ref": "../../schemas/base/spec-node.schema.json"
    }
  ],
  "properties": {
    "spec_node_id": {
      "const": "technology.artifact"
    },
    "layer_id": {
      "const": "technology"
    },
    "type": {
      "const": "artifact"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "artifactType": {
          "type": "string",
          "enum": [
            "database",
            "file",
            "configuration",
            "binary",
            "log",
            "backup"
          ]
        }
      },
      "required": [
        "artifactType"
      ],
      "additionalProperties": false
    }
  }
}
