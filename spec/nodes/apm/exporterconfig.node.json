{
  "id": "apm.exporterconfig",
  "layer_id": "apm",
  "name": "ExporterConfig",
  "description": "Configuration for telemetry data export destinations, specifying protocol (OTLP, Jaeger, Prometheus), endpoints, authentication, batching, and retry policies. Controls where observability data is sent.",
  "attributes": {
    "id": {
      "type": "string",
      "description": "",
      "format": "uuid"
    },
    "name": {
      "type": "string",
      "description": ""
    },
    "type": {
      "type": "enum",
      "description": "",
      "enum_values": [
        "otlp",
        "jaeger",
        "zipkin",
        "prometheus",
        "console",
        "file",
        "logging",
        "noop"
      ]
    },
    "enabled": {
      "type": "boolean",
      "description": ""
    },
    "endpoint": {
      "type": "string",
      "description": ""
    },
    "protocol": {
      "type": "enum",
      "description": "",
      "enum_values": [
        "grpc",
        "http",
        "http/json"
      ]
    },
    "auth_type": {
      "type": "enum",
      "description": "AuthType classification",
      "enum_values": [
        "none",
        "api-key",
        "bearer",
        "basic",
        "oauth2",
        "mTLS"
      ]
    },
    "compression": {
      "type": "enum",
      "description": "CompressionType classification",
      "enum_values": [
        "none",
        "gzip",
        "zstd"
      ]
    }
  },
  "required_attributes": [
    "id",
    "name",
    "type",
    "endpoint"
  ]
}
