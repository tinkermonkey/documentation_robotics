{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/tinkermonkey/documentation_robotics/spec/nodes/security/actorobjective.node.schema.json",
  "title": "ActorObjective",
  "description": "Security-related objective or goal of an actor",
  "allOf": [
    {
      "$ref": "../../schemas/base/spec-node.schema.json"
    }
  ],
  "properties": {
    "spec_node_id": {
      "const": "security.actorobjective"
    },
    "layer_id": {
      "const": "security"
    },
    "type": {
      "const": "actorobjective"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "criticality": {
          "type": "string",
          "enum": [
            "low",
            "medium",
            "high",
            "critical"
          ]
        }
      },
      "additionalProperties": false
    }
  },
  "required": [
    "description"
  ]
}
